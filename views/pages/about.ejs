
<%- contentFor('hero') %>
		<section class="hero">
			<div class="hero-content">
				<h2>All about us</h2>
				<p>We love technology and use it for good purpose.</p>

				<a href="/contact" class="btn btn-primary btn-lg">Drop Mail</a>
			</div>
		</section>

		<%- contentFor('body') %>
				<div class="jumbotron text-center">
					<h1>Core Team</h1>

					<div class="row">
						<% for (user of users) { %>
						<div class="col-sm-3">
							<h2><%= user.name %></h2>
							<img src="<%= user.avatar %>" alt="pussy" class="img-responsive">
						</div>
						<% } %>
					</div>
				</div>
			

		